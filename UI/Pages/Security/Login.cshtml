@page
@using DataBaseModel
@{
    Layout = null;
    var pageConfig = Config.pageConfig();
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Login</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <style> @@import url("https://fonts.googleapis.com/css2?family=Poppins:wght@200;300;400;500;600;700;800&display=swap");</style>
    <script>
        localStorage.setItem("TITULO", '@pageConfig.TITULO');
        localStorage.setItem("RUC", '@pageConfig.RUC');
        localStorage.setItem("SUB_TITULO", '@pageConfig.SUB_TITULO');
        localStorage.setItem("NOMBRE_EMPRESA", '@pageConfig.NOMBRE_EMPRESA');
        localStorage.setItem("MEDIA_IMG_PATH", '@pageConfig.MEDIA_IMG_PATH');
        localStorage.setItem("LOGO_PRINCIPAL", '@pageConfig.LOGO_PRINCIPAL');
        localStorage.setItem("INFO_TEL", '@pageConfig.INFO_TEL');
        localStorage.setItem("EMAIL", '@pageConfig.EMAIL');
    </script>
    <script src="~/WDevCore/Security/Login.js" type="module"></script>
</head>
<body id="App">
</body>
</html>